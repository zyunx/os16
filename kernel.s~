BITS 16
ORG 0x80000						; raw binary specific, specify the
								; address at which the output code
								; will eventually be loaded.

	jmp 0x80000:start_kernel	; gotot segment 0, offset 'start'


start_kernel:
	mov ax, cs
	mov ds, ax
	mov es, ax
	mov ss, ax
	mov sp, 0

	; print
	mov ah, 9
	mov al, '='
	mov bx, 7
	mov cx, 4
	int 0x10

hang:
	jmp $

putw:
	push bp
	mov ah, 9
	mov al, '='
	mov bx, 7
	mov cx, 4
	int 0x10

	
times 0x10000-($-$$) db 0
dw 0xaa55

